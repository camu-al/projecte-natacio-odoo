<odoo>
    <record id="view_natacio_category_list" model="ir.ui.view">
        <field name="name">natacio.category.list</field>
        <field name="model">natacio.category</field>
        <field name="arch" type="xml">
            <list string="Categories">
                <field name="name"/>
                <field name="age_min"/>
                <field name="age_max"/>
            </list>
        </field>
    </record>

    <record id="view_natacio_category_form" model="ir.ui.view">
        <field name="name">natacio.category.form</field>
        <field name="model">natacio.category</field>
        <field name="arch" type="xml">
            <form string="Categoria">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="age_min"/>
                        <field name="age_max"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_natacio_category_list" model="ir.actions.act_window">
        <field name="name">Categories</field>
        <field name="res_model">natacio.category</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
